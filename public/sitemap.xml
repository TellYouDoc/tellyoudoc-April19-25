<?xml version="1.0" encoding="UTF-8"?>
<!-- XML Sitemap - Search Engine Discovery File -->
<!-- This file helps search engines find and index all important pages on your website -->
<!-- It's crucial for SEO as it ensures search engines know about all your content -->

<!-- urlset - root element containing all URL entries -->
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  
  <!-- Homepage - highest priority page -->
  <url>
    <!-- loc - the actual URL of the page -->
    <loc>https://tellyoudoc.com/</loc>
    <!-- lastmod - when the page was last modified (YYYY-MM-DD format) -->
    <!-- Helps search engines know if content has changed -->
    <lastmod>2024-12-19</lastmod>
    <!-- changefreq - how often the page content changes -->
    <!-- "weekly" - content changes weekly, good for dynamic sites -->
    <changefreq>weekly</changefreq>
    <!-- priority - relative importance of this page (0.0 to 1.0) -->
    <!-- 1.0 = highest priority, homepage should always be 1.0 -->
    <priority>1.0</priority>
  </url>
  
  <!-- Login page - important for user engagement -->
  <url>
    <loc>https://tellyoudoc.com/login</loc>
    <lastmod>2024-12-19</lastmod>
    <!-- "monthly" - content changes monthly, typical for static pages -->
    <changefreq>monthly</changefreq>
    <!-- 0.8 - high priority but not as important as homepage -->
    <priority>0.8</priority>
  </url>
  
  <!-- Registration page - important for user acquisition -->
  <url>
    <loc>https://tellyoudoc.com/register</loc>
    <lastmod>2024-12-19</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.8</priority>
  </url>
  
  <!-- Partner page - business development content -->
  <url>
    <loc>https://tellyoudoc.com/partner</loc>
    <lastmod>2024-12-19</lastmod>
    <changefreq>monthly</changefreq>
    <!-- 0.7 - important but secondary to user-facing pages -->
    <priority>0.7</priority>
  </url>
  
  <!-- Privacy Policy - legal requirement, low priority for SEO -->
  <url>
    <loc>https://tellyoudoc.com/privacy</loc>
    <lastmod>2024-12-19</lastmod>
    <!-- "yearly" - legal pages rarely change -->
    <changefreq>yearly</changefreq>
    <!-- 0.3 - low priority, mainly for compliance -->
    <priority>0.3</priority>
  </url>
  
  <!-- Terms of Service - legal requirement, low priority for SEO -->
  <url>
    <loc>https://tellyoudoc.com/terms</loc>
    <lastmod>2024-12-19</lastmod>
    <changefreq>yearly</changefreq>
    <priority>0.3</priority>
  </url>
  
</urlset> 